/* bubble sort is the easiest sorting algorithm to memorize */
#include <stdio.h>
#include <stdlib.h>
#define SIZE 10
void bubblesort();
void printarray();
int a[SIZE]={54, 25, 44, 30, 77, 12, 22, 50, 32, 69};
int main(){
	bubblesort();
	printarray();
  system("pause");
	return 0;
}

void bubblesort(){
	int i, j, t;
	for (i=0;i<SIZE;i++){
		for (j=0;j<SIZE;j++){
			if (a[i]<a[j]){
				t=a[j];
				a[j]=a[i];
				a[i]=t;
			}
		}
	}
}

void printarray(){
	int i;
	for (i=0;i<SIZE;i++){
		printf("%d ", a[i]);
	}
	printf("\n");
}
